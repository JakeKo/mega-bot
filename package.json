{
  "name": "mega-bot",
  "version": "1.0.0",
  "description": "A fire-from-the-hip attempt at making a Discord bot",
  "main": "app.js",
  "scripts": {
    "lint": "eslint ./**/*.js",
    "build:dev": "cross-env NODE_ENV=development node ./app.js",
    "build:prod": "cross-env NODE_ENV=production node ./app.js",
    "dev": "npm run lint && npm run build:dev",
    "prod": "npm run build:prod",
    "start": "npm run prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JakeKo/mega-bot.git"
  },
  "author": "Jake Koperski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JakeKo/mega-bot/issues"
  },
  "homepage": "https://github.com/JakeKo/mega-bot#readme",
  "dependencies": {
    "cross-env": "^7.0.2",
    "discord.js": "^12.2.0",
    "mongodb": "^3.5.7"
  },
  "devDependencies": {
    "eslint": "^7.1.0"
  }
}
